{
  "name": "core/categories",
  "kubioSupport": {
	"elementsEnum": {
	  "WIDGET_BOX": "widget-box",
	  "WIDGET_LINK": "widget-link",
	  "WIDGET_ALIGN_HOLDER": "widget-align",
	  "WIDGET_LIST_ITEM": "widget-item",
	  "WIDGET_LIST": "widget-list",
	  "WIDGET_DROPDOWN": "widget-dropdown"
	},
	"elementsByName": {
	  "widget-box": {
		"default": true,
		"wrapper": true
	  },
	  "widget-link": {
		"selector": "a",
		"usePrefix": true
	  },
	  "widget-align": {
		"selector": "&.wp-block-categories",
		"usePrefix": true
	  },
	  "widget-item": {
		"selector": "&.wp-block-categories li",
		"usePrefix": true
	  },
	  "widget-list": {
		"selector": "&.wp-block-categories",
		"usePrefix": true
	  },
	  "widget-dropdown": {
		"selector": "&.wp-block-categories > select",
		"usePrefix": true
	  }
	}
  }
}
