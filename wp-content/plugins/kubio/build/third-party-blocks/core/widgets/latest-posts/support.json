{
  "name": "core/latest-posts",
  "kubioSupport": {
	"elementsEnum": {
	  "WIDGET_BOX": "widget-box",
	  "WIDGET_LINK": "widget-link",
	  "WIDGET_ALIGN_HOLDER": "widget-align",
	  "WIDGET_DATE": "widget-date",
	  "WIDGET_AUTHOR_NAME": "widget-author",
	  "WIDGET_CONTENT": "widget-content",
	  "WIDGET_LIST_ITEM": "widget-item",
	  "WIDGET_IMAGE": "widget-image"
	},
	"elementsByName": {
	  "widget-box": {
		"default": true,
		"wrapper": true
	  },
	  "widget-link": {
		"selector": "a",
		"usePrefix": true
	  },
	  "widget-date": {
		"selector": "time",
		"usePrefix": true
	  },
	  "widget-author": {
		"selector": ".wp-block-latest-posts__post-author",
		"usePrefix": true
	  },
	  "widget-content": {
		"selector": ".wp-block-latest-posts__post-excerpt",
		"usePrefix": true
	  },
	  "widget-align": {
		"selector": ".wp-block-latest-posts",
		"usePrefix": true
	  },
	  "widget-item": {
		"selector": "li",
		"usePrefix": true
	  },
	  "widget-image": {
		"selector": ".wp-block-latest-posts__featured-image",
		"usePrefix": true
	  }
	}
  }
}
